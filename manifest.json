{
  "name": "tnews-extension",
  "version": "1.0.0",
  "manifest_version": 3,
  "description": "chrome extension that disables the prefectures that are unnecessary",
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [{
    "matches": ["https://www.tnews.jp/*"],
    "run_at":"document_end",
    "js": [
      "content.js"
    ]
  }],
  "permissions": [
    "storage"
  ]
}